<div class='sq-loading' style='display: none; position:absolute; width:100%; top:40%; z-index: 1;'>
	<div class="spinner">
	<div class="rect5"></div>
	<div class="rect4"></div>
	<div class="rect3"></div>
	<div class="rect2"></div>
	<div class="rect1"></div>
	<div class="rect2"></div>
	<div class="rect3"></div>
	<div class="rect4"></div>
	<div class="rect5"></div>
	</div>
</div>
<div id="squid-api-data-widgets-data-table">
	<table class="sq-table">
		<thead>
			<tr></tr>
		</thead>
		<tbody></tbody>
	</table>
	<div id="error"></div>
	<div id="stale">
		<div class="reactiveMessage"><span><i class="fa fa-table"></i><br>
			{{staleMessage}}
		</span></div>
